<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SpellingReadingActivity">

  <TextView
      android:id="@+id/spellingReading"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="sans-serif-medium"
      android:text="AYO LATIHAN MEMBACA"
      android:textSize="25sp"
      android:textStyle="bold"
      android:layout_marginTop="30dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <TextView
      android:id="@+id/txt1"
      android:layout_width="60dp"
      android:layout_height="60dp"
      android:background="@drawable/circle"
      android:gravity="center"
      android:text="1"
      android:textStyle="bold"
      android:textSize="25sp"
      android:layout_marginStart="20dp"
      android:layout_marginTop="30dp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/spellingReading" />

  <TextView
      android:id="@+id/txt2"
      android:layout_width="60dp"
      android:layout_height="60dp"
      android:background="@drawable/circle"
      android:gravity="center"
      android:text="2"
      android:textSize="25sp"
      android:textStyle="bold"
      android:layout_marginStart="17dp"
      android:layout_marginTop="30dp"
      app:layout_constraintStart_toEndOf="@+id/txt1"
      app:layout_constraintTop_toBottomOf="@+id/spellingReading" />

  <TextView
      android:id="@+id/txt3"
      android:layout_width="60dp"
      android:layout_height="60dp"
      android:background="@drawable/circle"
      android:gravity="center"
      android:text="3"
      android:textSize="25sp"
      android:textStyle="bold"
      android:layout_marginStart="17dp"
      android:layout_marginTop="30dp"
      app:layout_constraintStart_toEndOf="@+id/txt2"
      app:layout_constraintTop_toBottomOf="@+id/spellingReading" />

  <TextView
      android:id="@+id/txt4"
      android:layout_width="60dp"
      android:layout_height="60dp"
      android:background="@drawable/circle"
      android:gravity="center"
      android:text="4"
      android:textSize="25sp"
      android:textStyle="bold"
      android:layout_marginStart="17dp"
      android:layout_marginTop="30dp"
      app:layout_constraintStart_toEndOf="@+id/txt3"
      app:layout_constraintTop_toBottomOf="@+id/spellingReading" />

  <TextView
      android:id="@+id/txt5"
      android:layout_width="60dp"
      android:layout_height="60dp"
      android:layout_marginStart="17dp"
      android:layout_marginTop="30dp"
      android:layout_marginEnd="20dp"
      android:background="@drawable/circle"
      android:gravity="center"
      android:text="5"
      android:textSize="25sp"
      android:textStyle="bold"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/txt4"
      app:layout_constraintTop_toBottomOf="@+id/spellingReading" />

  <TextView
      android:id="@+id/txt6"
      android:layout_width="60dp"
      android:layout_height="60dp"
      android:background="@drawable/circle"
      android:gravity="center"
      android:text="6"
      android:textSize="25sp"
      android:textStyle="bold"
      android:layout_marginStart="20dp"
      android:layout_marginTop="30dp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/txt1" />

  <TextView
      android:id="@+id/txt7"
      android:layout_width="60dp"
      android:layout_height="60dp"
      android:background="@drawable/circle"
      android:gravity="center"
      android:text="7"
      android:textSize="25sp"
      android:textStyle="bold"
      android:layout_marginStart="17dp"
      android:layout_marginTop="30dp"
      app:layout_constraintStart_toEndOf="@+id/txt6"
      app:layout_constraintTop_toBottomOf="@+id/txt2" />

  <TextView
      android:id="@+id/txt8"
      android:layout_width="60dp"
      android:layout_height="60dp"
      android:background="@drawable/circle"
      android:gravity="center"
      android:text="8"
      android:textSize="25sp"
      android:textStyle="bold"
      android:layout_marginStart="17dp"
      android:layout_marginTop="30dp"
      app:layout_constraintStart_toEndOf="@+id/txt7"
      app:layout_constraintTop_toBottomOf="@+id/txt3" />

  <TextView
      android:id="@+id/txt9"
      android:layout_width="60dp"
      android:layout_height="60dp"
      android:background="@drawable/circle"
      android:gravity="center"
      android:text="9"
      android:textSize="25sp"
      android:layout_marginStart="17dp"
      android:layout_marginTop="30dp"
      android:textStyle="bold"
      app:layout_constraintStart_toEndOf="@+id/txt8"
      app:layout_constraintTop_toBottomOf="@+id/txt4" />

  <TextView
      android:id="@+id/txt10"
      android:layout_width="60dp"
      android:layout_height="60dp"
      android:layout_marginStart="17dp"
      android:layout_marginTop="30dp"
      android:layout_marginEnd="20dp"
      android:background="@drawable/circle"
      android:gravity="center"
      android:text="10"
      android:textSize="25sp"
      android:textStyle="bold"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/txt9"
      app:layout_constraintTop_toBottomOf="@+id/txt5" />

</androidx.constraintlayout.widget.ConstraintLayout>